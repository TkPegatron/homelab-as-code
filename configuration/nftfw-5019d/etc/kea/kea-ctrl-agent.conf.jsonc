{
    "Control-agent": {
        "http-host": "172.22.0.120",
        "http-port": 8647,
        "control-sockets": {
            "dhcp4": {
                "socket-type": "unix",
                "socket-name": "/tmp/kea4-ctrl-socket"
            },
            "dhcp6": {
                "socket-type": "unix",
                "socket-name": "/tmp/kea6-ctrl-socket"
            },
            "d2": {
                "socket-type": "unix",
                "socket-name": "/tmp/kea-ddns-ctrl-socket"
            }
        },
        "loggers": [
            {
                "name": "kea-ctrl-agent",
                "severity": "INFO",
                "debuglevel": 0,
                "output_options": [
                    {
                        "output": "/var/log/kea-ctrl-agent.log",
                        // "pattern": "%-5p %m\n",
                        "pattern": "%d{%y.%m.%d %H:%M:%S.%q} %-5p [%c/%i] %m\n",
                        "flush": true,
                        "maxsize": 204800,
                        "maxver": 4
                    }
                ]
            }
        ]
    }
}
